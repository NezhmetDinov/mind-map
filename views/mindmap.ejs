<%- include("partials/header") %>


  <h3>Number of sources are <%= numberOfSources %></h3>
  <h3>Number of words are <%= numberOfWords %></h3>

  <% for (var i = 0; i < numberOfSources; i++) { %>
    <svg height="100" width="100">
      <circle cx="50" cy="50" r="40" stroke="white" stroke-width="2" fill="none" />
      <text x="50" y="50" text-anchor="middle" fill="white"><%= sourceNames.split(" ")[i] %></text>
    </svg>
  <% } %>
<br>
  <% for (var i = 0; i < numberOfWords; i++) { %>
    <svg height="100" width="100">
      <circle cx="50" cy="50" r="25" stroke="white" stroke-width="2" fill="none" />
      <text x="50" y="50" text-anchor="middle" fill="white"><%= wordNames.split(" ")[i] %></text>
    </svg>
  <% } %>

<%- include("partials/footer") %>
